
var Schema = {};
Schema.createSchema = function(mongoose)
{
    var AsgardiumResourceDataSchema = mongoose.Schema(
    {
        "resource_id" : {type:Number, "default":0},
        "name_strid" : {type:Number, "default":0},
        "icon_id" : {type:Number, "default":0},
        "value" : {type:Number, "default":0},
        "rid_fuel1" : {type:Number, "default":0},
        "rid_fuel2" : {type:Number, "default":0}
    });
    
    return AsgardiumResourceDataSchema;
};

module.exports = Schema;